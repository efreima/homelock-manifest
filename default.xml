<manifest>
<!-- Origins that get referenced when pulling in code via repo sync -->
	<remote name="github-qt5" fetch="https://github.com/meta-qt5/" />
	<remote name="github-origin" fetch="https://github.com/efreima/" />
	<remote name="yocto-origin" fetch="https://git.yoctoproject.org/git/" />
	<remote name="oe-base" fetch="https://git.openembedded.org/" />

    <!-- Remote Poky repo -->
	<project name="homelock-poky" remote="github-origin" path="." revision="master" >
	</project>

    <!-- Remote hLOS repo -->
	<project name="homeLock" remote="github-origin" path="layers" revision="integration" >
	</project>

    <include name="hlos-base-manifest.xml" />


</manifest>
                   
