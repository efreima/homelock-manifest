<manifest>
<!-- Origins that get referenced when pulling in code via repo sync -->
	<remote name="github-qt5" fetch="https://github.com/meta-qt5/" />
	<remote name="github-origin" fetch="https://github.com/efreima/" />
	<remote name="yocto-origin" fetch="git://git.yoctoproject.org/" />
	<remote name="oe-base" fetch="https://git.openembedded.org/" />
	<remote name="jumpnow" fetch="https://github.com/jumpnow/" />

    <!-- Remote Poky repos -->
	<project name="homelock-poky" remote="github-origin" path="." revision="dunfell" >
	</project>
	<project name="meta-yocto-bsp" remote="github-origin" path="layers/meta-yocto-bsp" revision="main" >
	</project>

    <!-- Remote hLOS repos -->
	<project name="meta-homelock" remote="github-origin" path="layers/meta-homelock" revision="integration" >
		<copyfile dest="init-homelock-build" src="preinit/init-homelock-build" />
	</project>
	<project name="homelock-scripts" remote="github-origin" path="homelock-scripts" revision="master" >
        </project>


    <include name="hlos-base-manifest.xml" />


</manifest>
                   
